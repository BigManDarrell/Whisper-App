<%- include("partials/header") -%>
<div class="container-fluid w-100 p-5 text-center">
  <form action="/logout" method="post">
    <input
      type="submit"
      name="logout"
      value="Log Out"
      class="logout-btn float-start"
    />
  </form>
  <a href="/submit" class="login-btn float-end">Submit a Secret</a>
  <img src="images/key.png" alt="" width="100" />
  <h1 class="mt-5 mb-2">You've discovered MySecret!</h1>
  <%users.forEach(user=>{%>
  <p class="text-bg-dark fs-3 secret"><%= user.secret %></p> 
  <%})%>
</div>
<%- include("partials/footer") -%>
